#region snippet_ModelIsAdmin
<td>
    @if (Model.IsAdmin)
    {
    <a asp-page="Edit" asp-route-id="@product.Id">Edit</a> @:|
    <a href="#" onclick="deleteProduct('@product.Id', antiForgeryToken())">Delete</a>
    }
</td>
#endregion

#region snippet_AddProductLink
