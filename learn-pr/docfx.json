{
  "build": {
    "content": [
      {
        "files": [
          "achievements.yml"
        ],
        "dest": "achievements/main/"
      },
      {
        "src": "azure",
        "files": [
          "**.yml"
        ],
        "dest": "modules"
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
          "azure/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.gif",
          "**/*.png",
          "**/*.svg",
          "**/*.jpg",
          "**/*.jpeg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/learn/breadcrumb/toc.json",
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/learn",
      "clicktale": true,
      "uhfHeaderId":"MSDocsHeader-Learn"
    },
    "fileMetadata": {
      "brand": {
        "azure/**/*.yml": "azure"
      },
      "sandbox-disabled": {
         "azure/welcome-to-azure/**/*.yml": "true",
         "azure/automate-azure-tasks-with-powershell/**/*.yml": "true",
         "azure/build-a-web-app-with-mean-on-a-linux-vm/**/*.yml": "true",
         "azure/caching-and-performance-azure-storage-and-disks/**/*.yml": "true",
         "azure/create-linux-virtual-machine-in-azure/**/*.yml": "true",
         "azure/create-windows-virtual-machine-in-azure/**/*.yml": "true",
         "azure/intro-to-azure-virtual-machines/**/*.yml": "true",
         "azure/make-your-virtual-machines-highly-available/**/*.yml": "true",
         "azure/manage-virtual-machines-with-azure-cli/**/*.yml": "true",
         "azure/add-and-size-disks-in-azure-virtual-machines/**/*.yml": "true",
         "azure/secure-your-azure-virtual-machine-disks/**/*.yml": "true",
         "azure/intro-to-containers/**/*.yml": "true",
         "azure/ci-cd-pipelines-azure-devops/**/*.yml": "true",
         "azure/create-computer-vision-service-to-classify-images/**/*.yml": "true",
         "azure/interactive-deep-learning/**/*.yml": "true"
        }
    },
    "template": [],
    "dest": "learn-pr",
    "markdownEngineName": "markdig"
  }
}
